FRONTEND_URL=http://localhost:3000/
JHAAS_DOMAIN=jhaas.local
RELEASE_NAME=jhaas-portal

POSTGRES_HOST=jhaas-portal-db
POSTGRES_PORT=5432
TYPEORM_DB_SYNC=false
TYPEORM_DB_LOGGING=false

# S3 configuration for system files (state/specs)
S3_ENDPOINT=minio
S3_PORT=9000
S3_SSL=false
S3_ACCESS_KEY=minio
S3_SECRET_KEY=super-secret-secret
S3_JH_SPECS_BUCKET=jh-specs

# S3 configuration for notebook data mounts
S3_DATA_ENDPOINT=minio
S3_DATA_PORT=9000
S3_DATA_SSL=false
S3_DATA_ACCESS_KEY=minio

OIDC_ENDPOINT=http://authentik:9000/application/o/portal
OIDC_URL_CALLBACK=http://localhost:3000/api/auth/oidc/cb
OIDC_CLIENT_ID=jhaas-portal
OIDC_CLIENT_SECRET=mysupersecretclientsecretmysupersecretclientsecret
OIDC_FORCE_REACHABILITY=true

AUTHENTIK_API_ENDPOINT=http://authentik:9000/api/v3
AUTHENTIK_API_SECRET=tokentokentokentokentokentokentokentokentokentoken
AUTHENTIK_INVALIDATION_FLOW=http://authentik:9000/if/flow/logout/
AUTHENTIK_JH_GROUP_NAME=jupyterhubs

SESSION_COOKIE_NAME=session
SESSION_COOKIE_PATH=/
SESSION_COOKIE_MAX_AGE=28800000
SESSION_COOKIE_SECRET=["super-secret-1", "super-secret-2", "super-secret-X"]
SESSION_COOKIE_SECURE=false

SESSION_STORAGE=redis
SESSION_STORAGE_URL=redis://redis
SESSION_STORAGE_USER=
SESSION_STORAGE_PASS=
SESSION_STORAGE_PREFIX=

K8S_TF_IMAGE=hashicorp/terraform
K8S_TF_IMAGE_PP=IfNotPresent
K8S_TF_NS=terraform

MAIL_HOST=mailhog
MAIL_PORT=2525
MAIL_SECURE=false
MAIL_TEMPLATE_DIR=
MAIL_FROM=portal@jhaas.local
MAIL_COPY_ADDRESSES=["admins@jhaas.local"]
